oauth.wso2.clientid=<%= scope.lookupvar('multi_server::api_instance::install::wso2_client_id') %>
oauth.wso2.clientsecret=<%= scope.lookupvar('multi_server::api_instance::install::wso2_secret') %>
oauth.wso2.redirecturis=<%= scope.lookupvar('multi_server::api_instance::install::host_protocol') %>://<%= scope.lookupvar('multi_server::api_instance::install::host_url') %>/api/oauth/callback
oauth.wso2.authuri=https://identity.cloud.wso2.com:443/oauth2/authorize
oauth.wso2.tokenuri=https://identity.cloud.wso2.com:443/oauth2/token
oauth.wso2.useruri=https://identity.cloud.wso2.com:443/oauth2/userinfo
