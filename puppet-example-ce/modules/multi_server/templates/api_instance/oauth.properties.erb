oauth.google.clientid=<%= scope.lookupvar('multi_server::api_instance::install::google_client_id') %>
oauth.google.clientsecret=<%= scope.lookupvar('multi_server::api_instance::install::google_secret') %>
oauth.google.redirecturis=<%= scope.lookupvar('multi_server::api_instance::install::host_protocol') %>://<%= scope.lookupvar('multi_server::api_instance::install::host_url') %>/api/oauth/callback
oauth.google.authuri=NULL
oauth.google.tokenuri=NULL

oauth.github.clientid=<%= scope.lookupvar('multi_server::api_instance::install::github_client_id') %>
oauth.github.clientsecret=<%= scope.lookupvar('multi_server::api_instance::install::github_secret') %>
oauth.github.redirecturis=<%= scope.lookupvar('multi_server::api_instance::install::host_protocol') %>://<%= scope.lookupvar('multi_server::api_instance::install::host_url') %>/api/oauth/callback
